#<---------------------------------------- 100 Characters ---------------------------------------->#

function scad() {
    DQ='"' # DQ == Double Quote
    FOO="name=$DQ$1$DQ" # $1 == the value to set name to
    openscad "$HR2_DIRECTORY/mechanical/hr2_models.scad" -D $FOO
}
case "$1" in
    "Blue_20x20_Grove")
	scad "Blue_20x20_Grove"
	;;
    "LED")
	scad "LED"
	;;
    "encoder_2xm1x3ra_bare")
	scad "encoder_2xm1x3ra_bare"
	;;
    "encoder_2xm1x3ra_board")
	scad "encoder_2xm1x3ra_board"
	;;
    "expansion_flat")
	scad "expansion_flat"
	;;
    "expansion_plate")
	scad "expansion_plate"
	;;
    "f1x4lp")
	scad "f1x4lp"
	;;
    "hr2_arm_assembly")
	scad "hr2_arm_assembly"
	;;
    "hr2_base_assembly")
	scad "hr2_base_assembly"
	;;
    "hr2_master_assembly")
	scad "hr2_master_assembly"
	;;
